{
  "version": 2,
  "name": "findlanqbot-backend-api",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/rag/upload",
      "dest": "/api/rag/upload.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/finlandq",
      "dest": "/public/finlandq.html"
    },
    {
      "src": "/upload",
      "dest": "/public/upload.html"
    },
    {
      "src": "/upload-text",
      "dest": "/public/upload-text.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
} 